.featured_section {
  display: inline-block;
  position: relative;
  z-index: 1;
}
.featured_section .service_heading {
  display: inline-block;
  width: 100%;
  margin: 39px 0 30px 0;
}
.featured_section .service_heading h2 {
  font-size: 36px;
  color: #2f2727;
  float: left;
  text-transform: uppercase;
  font-weight: 600;
  position: relative;
}
.featured_section .service_heading h2:after {
  content: '';
  width: 8px;
  height: 8px;
  display: inline-block;
  background: #739ab4;
  position: absolute;
  bottom: -20px;
  left: 4px;
  transform: rotate(45deg);
}
.featured_section .service_heading h2:before {
  content: '';
  width: 100px;
  height: 4px;
  display: inline-block;
  background: #739ab4;
  position: absolute;
  bottom: -19px;
  left: 37px;
}
.featured_section .service_heading h2 span {
  font-weight: normal;
  color: #2f2727;
  font-size: 16px;
  display: block;
  margin-bottom: 7px;
}
.featured_section .service_heading h3 {
  font-size: 18px;
  color: #a3a3a3;
  float: right;
  max-width: 430px;
  font-style: oblique;
  font-weight: normal;
  line-height: 24px;
}
.featured_section .block_featured {
  display: inline-block;
}
.featured_section .block_featured .category_block {
  width: 48%;
  float: left;
  margin-top: 30px;
  position: relative;
}
.featured_section .block_featured .category_block:nth-child(even) {
  float: right;
}
.featured_section .block_featured {
  text-align: center;
}
.featured_section .block_featured .product_img {
  border: solid 1px #e2e2e2;
  border-radius: 10px;
  overflow: hidden;
  display: inline-block;
  position: relative;
}
.featured_section .block_featured .product_img .hover_block {
  display: inline-block;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
  display: none;
}
.featured_section .block_featured .product_img .hover_block:after {
  content: '';
  background: rgba(116, 154, 180, 0.78);
  width: 100%;
  height: 100%;
  display: inline-block;
}
.featured_section .block_featured .product_img .hover_block a {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 1;
  color: #045270;
  font-size: 16px;
  background: #FFF;
  text-transform: uppercase;
  padding: 11px 33px;
  border-radius: 23px;
  text-decoration: none;
}
.featured_section .block_featured .product_img .hover_block a:hover {
  background: #333;
  color: #FFF;
}
.featured_section .block_featured .product_img:hover .hover_block {
  display: inline-block;
}
.featured_section .block_featured h4 {
  color: #8b9291;
  font-size: 20px;
  font-weight: normal;
  text-transform: uppercase;
  margin: 11px 0 0 0;
}
